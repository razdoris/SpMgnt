{% extends 'admin/adminBase.html.twig' %}

{% block title %}{{ parent() }} | Détails de la location de la blessure{% endblock %}

{% block body %}
    <h1>Détails de la location de la blessure</h1>

    <table class="tableAdminDetail">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ application_player_injury_location.id }}</td>
            </tr>
            <tr>
                <th>Localisation</th>
                <td>{{ application_player_injury_location.location }}</td>
            </tr>
            <tr>
                <th>Lateralite</th>
                <td>{{ application_player_injury_location.laterality }}</td>
            </tr>
        </tbody>
    </table>


    {% set pathUrl =  'admin_application_player_injury_location'%}
    {% set IdObject = application_player_injury_location.id %}
    {{ include('admin/_formCRUD/_delete_form.html.twig',{'pathUrl': pathUrl, 'id':  IdObject}) }}
    {{ include('admin/_inc/_footerShow.html.twig',{'pathUrl': pathUrl, 'id':  IdObject}) }}

{% endblock %}
