{% extends 'admin/adminBase.html.twig' %}

{% block title %}{{ parent() }} | Détails de la disponibilite{% endblock %}

{% block body %}
    <h1>Détails de la disponibilite</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ application_player_disponibility.id }}</td>
            </tr>
            <tr>
                <th>Label</th>
                <td>{{ application_player_disponibility.label }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ application_player_disponibility.type }}</td>
            </tr>
            <tr>
                <th>Color</th>
                <td>{{ application_player_disponibility.color }}</td>
            </tr>
        </tbody>
    </table>


    {% set pathUrl =  'admin_application_player_disponibility'%}
    {% set IdObject = application_player_disponibility.id %}
    {{ include('admin/_formCRUD/_delete_form.html.twig',{'pathUrl': pathUrl, 'id':  IdObject}) }}
    {{ include('admin/_inc/_footerShow.html.twig',{'pathUrl': pathUrl, 'id':  IdObject}) }}

{% endblock %}
