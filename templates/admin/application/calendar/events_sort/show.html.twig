{% extends 'admin/adminBase.html.twig' %}

{% block title %}{{ parent() }} | Détails type d'evenement{% endblock %}


{% block body %}
    <h1>Détail de type d'evenement :</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ application_calendar_events_sort.id }}</td>
            </tr>
            <tr>
                <th>Label</th>
                <td>{{ application_calendar_events_sort.label }}</td>
            </tr>
            <tr>
                <th>Icon</th>
                <td>{{ application_calendar_events_sort.icon }}</td>
            </tr>
            <tr>
                <th>BackgroundColor</th>
                <td>{{ application_calendar_events_sort.backgroundColor }}</td>
            </tr>
            <tr>
                <th>BorderColor</th>
                <td>{{ application_calendar_events_sort.borderColor }}</td>
            </tr>
            <tr>
                <th>TextColor</th>
                <td>{{ application_calendar_events_sort.TextColor }}</td>
            </tr>
        </tbody>
    </table>


    {% set pathUrl =  'admin_application_calendar_events_sort'%}
    {% set IdObject = application_calendar_events_sort.id %}
    {{ include('admin/_formCRUD/_delete_form.html.twig',{'pathUrl': pathUrl, 'id':  IdObject}) }}
    {{ include('admin/_inc/_footerShow.html.twig',{'pathUrl': pathUrl, 'id':  IdObject}) }}


{% endblock %}
