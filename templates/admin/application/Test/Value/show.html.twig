{% extends 'admin/adminBase.html.twig' %}

{% block title %}{{ parent() }} | Détails de la valeur du test{% endblock %}

{% block body %}
    <h1>Détails de la valeur du test</h1>

    <table class="tableAdminDetail">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ application_test_value.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ application_test_value.name }}</td>
            </tr>
            <tr>
                <th>Unit</th>
                <td>{{ application_test_value.unit }}</td>
            </tr>
        </tbody>
    </table>


    {% set pathUrl =  'admin_application_test_value'%}
    {% set IdObject = application_test_value.id %}
    {{ include('admin/_formCRUD/_delete_form.html.twig',{'pathUrl': pathUrl, 'id':  IdObject}) }}
    {{ include('admin/_inc/_footerShow.html.twig',{'pathUrl': pathUrl, 'id':  IdObject}) }}

{% endblock %}
