{% block title %}Détails caractéristique{% endblock %}

{% block body %}
    <h1>détails de la caractéristique de l'offre</h1>

    <table class="tableAdminDetail">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ presentation_offer_feature.id }}</td>
            </tr>
            <tr>
                <th>FeatureName</th>
