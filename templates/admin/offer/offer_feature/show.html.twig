{% extends 'admin/adminBase.html.twig' %}

{% block title %}{{ parent() }} | Détails caractéristique{% endblock %}

{% block body %}
    <h1>Détails de la caractéristique de l'offre</h1>

    <table class="tableAdminDetail">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ presentation_offer_feature.id }}</td>
            </tr>
            <tr>
                <th>FeatureName</th>
                <td>{{ presentation_offer_feature.featureName }}</td>
            </tr>
            <tr>
                <th>FeatureDescription</th>
                <td>{{ presentation_offer_feature.description|raw }}</td>
            </tr>
        </tbody>
    </table>

    {% set pathUrl =  'admin_offer_feature'%}
    {% set IdObject = presentation_offer_feature.id %}
    {{ include('admin/_formCRUD/_delete_form.html.twig',{'pathUrl': pathUrl, 'id':  IdObject}) }}
    {{ include('admin/_inc/_footerShow.html.twig',{'pathUrl': pathUrl, 'id':  IdObject}) }}


{% endblock %}
