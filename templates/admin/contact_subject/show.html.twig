{% extends 'base.html.twig' %}

{% block title %}{{ parent() }} | détails sujet{% endblock %}

{% block body %}
    <h1>Détails du sujet du formulaire de contact</h1>

    <table class="tableAdminDetail">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ contact_subject.id }}</td>
            </tr>
            <tr>
                <th>SubjectText</th>
                <td>{{ contact_subject.subjectText }}</td>
            </tr>
        </tbody>
    </table>

    {{ include('admin/_formCRUD/_delete_form.html.twig',{'pathUrl': 'admin_contact_subject_delete', 'id':  contact_subject.id}) }}

    <div class="linkAdmin">
        <a href="{{ path('admin_contact_subject_index') }}">back to list</a>
        <a href="{{ path('admin_contact_subject_edit', {'id': contact_subject.id}) }}">edit</a>
    </div>
{% endblock %}
