{% extends 'admin/adminBase.html.twig' %}

{% block title %}{{ parent() }} | Détails sujet{% endblock %}

{% block body %}
    <h1>Détails du sujet du formulaire de contact</h1>

    <table class="tableAdminDetail">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ contact_subject.id }}</td>
            </tr>
            <tr>
                <th>SubjectText</th>
                <td>{{ contact_subject.subjectText }}</td>
            </tr>
        </tbody>
    </table>

    {% set pathUrl =  'admin_contact_subject'%}
    {% set IdObject = contact_subject.id %}
    {{ include('admin/_formCRUD/_delete_form.html.twig',{'pathUrl': pathUrl, 'id':  IdObject}) }}
    {{ include('admin/_inc/_footerShow.html.twig',{'pathUrl': pathUrl, 'id':  IdObject}) }}


{% endblock %}
