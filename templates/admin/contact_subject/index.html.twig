{% extends 'base.html.twig' %}

{% block title %}ContactSubject index{% endblock %}

{% block body %}
    <h1>ContactSubject index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>SubjectText</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for contact_subject in contact_subjects %}
            <tr>
                <td>{{ contact_subject.id }}</td>
                <td>{{ contact_subject.subjectText }}</td>
                <td>
                    <a href="{{ path('admin_contact_subject__show', {'id': contact_subject.id}) }}">show</a>
                    <a href="{{ path('admin_contact_subject__edit', {'id': contact_subject.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('admin_contact_subject__new') }}">Create new</a>
{% endblock %}
